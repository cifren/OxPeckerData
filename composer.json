{
    "name": "cifren/oxpecker-data",
    "type": "symfony-bundle",
    "description": "Create quickly report tables with OxPeckerData, add-on for symfony",
    "keywords": ["reporting", "report", "data"],
    "homepage": "https://github.com/cifren/OxPeckerData",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis Le Coq",
            "email": "flecoq@earls.ca"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "knplabs/etl": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "symfony/phpunit-bridge": "^4.2",
        "friendsofphp/php-cs-fixer": "^2.14",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload":     {
        "psr-0": { "Cifren\\OxPeckerData": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Cifren/OxPeckerData",    
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
