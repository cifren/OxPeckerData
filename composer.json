{
    "name": "cifren/oxpecker-data",
    "type": "symfony-bundle",
    "description": "Create quickly report tables with OxPeckerData, add-on for symfony",
    "keywords": ["reporting", "report", "data"],
    "homepage": "https://github.com/cifren/OxPeckerData",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis Le Coq",
            "email": "flecoq@earls.ca"
        }
    ],
    "require": {
        "php": "^7.2",
        "knplabs/etl": "*",
        "psr/log": "*",
        "symfony/stopwatch": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "friendsofphp/php-cs-fixer": "*",
        "php-coveralls/php-coveralls": "*"
    },
    "autoload":     {
        "psr-0": { "Cifren\\OxPeckerData": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Cifren/OxPeckerData",    
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
